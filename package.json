{
  "name": "firebase-secrets-cli",
  "version": "1.0.2",
  "type": "module",
  "bin": {
    "firebase-secrets": "./dist/firebase-secrets.js",
    "cic-index": "./dist/cic-index.js",
    "cic-bump": "./dist/cic-bump.js",
    "cic-push": "./dist/cic-push.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "npm run clean && tsc -p tsconfig.json",
    "pack": "npm run build && npm pack",
    "pack-bump": "npm run build && node scripts/bump-version.cjs && npm pack",
    "cic-push": "npm run build && npm pack && node scripts/git-push.cjs",
    "deploy": "npm run build && node scripts/bump-version.cjs && npm pack && node scripts/git-push.cjs && npm publish --access public",
    "only-deploy": "npm publish --access public",
    "deploy-no-bump": "npm run build && npm pack && node scripts/git-push.cjs && npm publish --access public",
    "start": "npm run build && node dist/cic-index.js"
  },
  "dependencies": {
    "@inquirer/select": "^5.0.4",
    "inquirer": "^13.2.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntoCic/firebase-secrets-cli.git"
  },
  "homepage": "https://github.com/AntoCic/firebase-secrets-cli#readme",
  "bugs": {
    "url": "https://github.com/AntoCic/firebase-secrets-cli/issues"
  }
}
